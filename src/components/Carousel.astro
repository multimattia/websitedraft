  <ImageWrapper slot="image-1" image={surcyclage} alt="logo for surcyclage" />
  <ImageWrapper slot="image-2" image={surcyclage} alt="logo for surcyclage" />
  <ImageWrapper slot="image-3" image={surcyclage} alt="logo for surcyclage" />
  <ImageWrapper slot="image-4" image={surcyclage} alt="logo for surcyclage" />
</Flick>
